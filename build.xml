<?xml version="1.0"?>

<project name="preconditions" basedir="." default="gen-func">
    <property name="path" value="bin/funcgen.php"/>
    <target name="gen-func" description="Generates function file.">
        <exec executable="php" outputProperty="result">
            <arg path="${path}"/>
        </exec>
        <echo message="${result}"/>
    </target>
</project>
